{
  "Logging": {
    "Name": {
      "Locale": "en-US",
      "Text": "Logging"
    },
    "Description": {
      "Locale": "en-US",
      "Text": "Everything related to logging"
    },
    "AuditLevel": {
      "Name": {
        "Locale": "en-US",
        "Text": "Audit Level"
      },
      "Description": {
        "Locale": "en-US",
        "Text": "Audit Level, use Syslog Enum entries only!"
      },
      "Value": "warning",
      "DefaultValue": "warning",
      "Type": "String",
      "Validation": {
        "Values": [
          "debug",
          "informational",
          "notice",
          "warning",
          "error",
          "critical",
          "alert",
          "emergency"
        ]
      }
    },
    "LogType": {
      "Name": {
        "Locale": "en-US",
        "Text": "Logging type"
      },
      "Description": {
        "Locale": "en-US",
        "Text": "Currently used to enable / disable log to storage. Enum"
      },
      "Value": "disabled",
      "DefaultValue": "disabled",
      "Type": "String",
      "Validation": {
        "Values": [
          "enabled",
          "disabled",
          "endpoint"
        ]
      }
    },
    "LogFileSize": {
      "Name": {
        "Locale": "en-US",
        "Text": "Size of Logfile (bytes)"
      },
      "Description": {
        "Locale": "en-US",
        "Text": "After this size, the logfile will cycle to the next one"
      },
      "Value": "250000",
      "DefaultValue": "250000",
      "Type": "Number",
      "Unit": "byte",
      "Validation": {
        "min": 1024
      }
    }
  },
  "Registry": {
    "Name": {
      "Locale": "en-US",
      "Text": "Registry"
    },
    "Description": {
      "Locale": "en-US",
      "Text": "Everything related to registry specific config"
    },
    "DevelopmentMode": {
      "Name": {
        "Locale": "en-US",
        "Text": "Development mode"
      },
      "Description": {
        "Locale": "en-US",
        "Text": "Enables or disables the development mode of the frontend"
      },
      "Value": "false",
      "DefaultValue": "false",
      "Type": "Boolean"
    },
    "ShowRegistry": {
      "Name": {
        "Locale": "en-US",
        "Text": "Show Registry"
      },
      "Description": {
        "Locale": "en-US",
        "Text": "Decides, whether to save the Registry along the other assets or not"
      },
      "Value": "true",
      "DefaultValue": "true",
      "Type": "Boolean"
    }
  },
  "Context": {
    "Name": {
      "Locale": "en-US",
      "Text": "Registry settings"
    },
    "Description": {
      "Locale": "en-US",
      "Text": "Settings for the OEC registry"
    }
  }
}
