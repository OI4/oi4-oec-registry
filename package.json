{
  "name": "oi4registry",
  "version": "0.12.1",
  "description": "A Registry that keeps track of all oi4 compatible members \u001b[D\u001b[A\u001b[Bmessagebus",
  "main": "index.js",
  "dependencies": {
    "@oi4/json-schemas": "^0.12.0"
  },
  "devDependencies": {},
  "scripts": {
    "testLocalUI": "echo \"Check your browser for port 5000\" && npx serve OI4-Local-UI/build -s",
    "buildLocalUI": "cd ./OI4-Local-UI && npm run build",
    "docker:prepare": "docker buildx create --name oi4-builder --driver docker-container --use --platform linux/amd64,linux/arm64,linux/arm/v7,linux/arm/v6,linux/386",
    "docker:build": "docker buildx build --push --platform linux/amd64,linux/arm64,linux/arm/v7,linux/arm/v6,linux/386 -t ghcr.io/oi4/oi4-registry:${npm_package_version} .",
    "docker:build:local": "docker build -t oi4/oi4-registry:${npm_package_version} -t oi4/oi4-registry:latest .",
    "serveLocalUI": "cd ./OI4-Local-UI && serve build/",
    "buildAll": "node ./Helper/UpdateVersion/index && cd ./OI4-Local-UI && npm run build && cd ../OI4-Service && npm run tsc && cd ..",
    "installAll": "cd ./OI4-Local-UI && npm install && cd ../OI4-Service && npm install && cd ..",
    "docker:prepare": "docker buildx create --name oi4-builder --driver docker-container --use --platform linux/amd64,linux/arm64,linux/arm/v7,linux/386",
    "docker:build": "docker buildx build --push --platform linux/amd64,linux/arm64,linux/arm/v7,linux/386 -t ghcr.io/oi4/oi4-registry:${npm_package_version} .",
    "docker:build:local": "docker build -t oi4/oi4-registry:${npm_package_version} -t oi4/oi4-registry:latest ."
  },
  "author": "Hilscher",
  "license": "MIT"
}
