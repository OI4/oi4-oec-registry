{
    "logging": {
        "name": {
            "locale": "en-US",
            "text": "Logging"
        },
        "description": {
            "locale": "en-US",
            "text": "Everything related to logging"
        },
        "auditLevel": {
            "name": {
                "locale": "en-US",
                "text": "Audit Level"
            },
            "description": {
                "locale": "en-US",
                "text": "Audit Level, use Syslog Enum entries only!"
            },
            "value": "warning",
            "defaultValue": "warning",
            "type": "String",
            "validation": {
                "values": [
                    "debug",
                    "informational",
                    "notice",
                    "warning",
                    "error",
                    "critical",
                    "alert",
                    "emergency"
                ]
            }
        },
        "logType": {
            "name": {
                "locale": "en-US",
                "text": "Logging type"
            },
            "description": {
                "locale": "en-US",
                "text": "Currently used to enable / disable log to storage. Enum"
            },
            "value": "disabled",
            "defaultValue": "disabled",
            "type": "String",
            "validation": {
                "values": [
                    "enabled",
                    "disabled",
					"endpoint"
                ]
            }
        },
        "logFileSize": {
            "name": {
                "locale": "en-US",
                "text": "Size of Logfile (bytes)"
            },
            "description": {
                "locale": "en-US",
                "text": "After this size, the logfile will cycle to the next one"
            },
            "value": "250000",
            "defaultValue": "250000",
            "type": "Number",
            "unit": "byte",
            "validation": {
                "min": 1024
            }
        }
    },
    "registry": {
        "name": {
            "locale": "en-US",
            "text": "Registry"
        },
        "description": {
            "locale": "en-US",
            "text": "Everything related to registry specific config"
        },
        "developmentMode": {
            "name": {
                "locale": "en-US",
                "text": "Development mode"
            },
            "description": {
                "locale": "en-US",
                "text": "Enables or disables the development mode of the frontend"
            },
            "value": "false",
            "defaultValue": "false",
            "type": "Boolean"
        },
        "showRegistry": {
            "name": {
                "locale": "en-US",
                "text": "Show Registry"
            },
            "description": {
                "locale": "en-US",
                "text": "Decides, whether to save the Registry along the other assets or not"
            },
            "value": "true",
            "defaultValue": "true",
            "type": "Boolean"
        }
    }
}